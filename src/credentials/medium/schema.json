{
  "$id": "https://sparks.foundation/schemas/medium-account.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "name": "MediumAccount",
  "description": "Medium Account Credential",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "handle": {
          "type": "string"
        }
      },
      "required": ["handle"]
    },
    "issued": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["handle", "issued"]
}
