{
  "$id": "https://example.com/schemas/email-account.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "name": "Email Account",
  "description": "Email Account Credential",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        }
      },
      "required": ["email"]
    },
    "issued": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["email", "issued"]
}
